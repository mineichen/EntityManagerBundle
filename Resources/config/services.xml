<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="mineichen_EntityManager.class">mineichen\entityManager\EntityManager</parameter>
        <parameter key="mineichen_EntityManagerFactory.class">mineichen\entityManager\ManagerFactory</parameter>
    </parameters>

    <services>
        <service id="mineichen_entitymanagerfactory"
                 class="%mineichen_EntityManagerFactory.class%" />

        <service id="mineichen_EntityManager"
                 class="%mineichen_EntityManager.class%" />

    </services>
</container>
